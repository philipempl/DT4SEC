  hash_id        = {b80d8cac876d948a184b7e60513d757b86885aeef8a18b44d318d917e32e1c63,ee96f00c06e673a3262a33c60b95a2a377ba90d93c52c1a56a62ff662b876962,513cd179de16b1c4f59dfcbdaddf6e3096df8092f2a7c3fa5ef3fe8bb2a627b7},
  keywords       = {Cloud security, Digital twin prototype, Digital twin instance, Prognostics, Skyhigh security, Virtualization, Internet of things},
}

