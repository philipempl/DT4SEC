@InProceedings{Lopatina2021,
  author = {Lopatina, Kate and Dokuchaev, V. A. and Maklachkova, V. V.},
  booktitle      = {2021 International Conference on Engineering Management of Communication and Technology (EMCTECH)},
  title  = {Data Risks Identification in Healthcare Sensor Networks},
  year   = {2021},
  pages  = {1--7},
  abstract       = {This article discusses the problem of studying the risks associated with information and communication systems of medical organizations in the context of their digital transformation and with the storage of medical data in a cloud geographically distributed infrastructure. Information about the health status of patients, their personal data and the results of medical research are classified as confidential information, which is the object of increased attention of cybercriminals. The widespread transformation of healthcare organizations, the creation of Internet of Medical Things (IoMT) ecosystems based on wireless sensor networks are increasing the number of potential risks of confidential information leakage. Medical data requires constant protection. The problem of identifying risks in order to develop methods and tools for their management in the future is further complicated by the fact that Healthcare Industry is increasingly beginning to use the cloud architecture of the IoMT, which in turn increases the potential threats to patients' confidential information. In the context of the COVID-19 pandemic, the number of ransomware programs that are being introduced into IoMT devices has increased, cyberattacks are becoming more complex with a significantly higher risk of fatal consequences for patients due to changes in the technical parameters of medical equipment by attackers. This makes it urgent to solve the problem of identifying possible risks in IoMT tools and systems, assessing their impact and possible consequences from unauthorized actions of intruders for patients and the medical organization as a whole.},
  doi    = {10.1109/EMCTECH53459.2021.9619178},
  hash_id        = {6023b3173c40b6a18423a7f560b8ccfd9a5e8a33a3e4882135a56d43de04dd9f},
  keywords       = {COVID-19;Cloud computing;Wireless sensor networks;Regulators;Distributed databases;Organizations;Medical services;Data;Healthcare;Risks Identification;Internet of Medical Things;medical organization;IoT;IoMT;Wireless Sensor Network;threats;digital twin;data protection;security;privacy;risk;models;system;information quality},
}

@InProceedings{Loper2019,
  author = {Loper, Margaret L. and Bronston, Marshall and Berenbach, Brian},
  booktitle      = {2019 Winter Simulation Conference (WSC)},
  title  = {Modeling &Amp; Simulation in Systems Engineering: A Survey Course},
  year   = {2019},
  pages  = {3295–3306},
  series = {WSC '19},
  abstract       = {Systems engineering is a bit different from other engineering disciplines in that students from many disciplines are enrolled in the program. Therefore, the objective is not to teach a simulation subject in depth, but rather to introduce the students to different techniques so that they can work with and manage simulation staff on a project. However, they need some "hands on" experience so that they know how challenging simulations can be, avoiding the trap of underestimating the effort involved. This paper describes the approach used at Georgia Tech to teach a compressed 7 week simulation survey course called ASE 6003 Modeling &amp; Simulation in Systems Engineering. We describe the techniques used, our approach and the results achieved over recent years of teaching simulation in this format. Finally we discuss lessons learned and offer suggestions for others interested in offering a similar course.},
  doi    = {10.1109/wsc40007.2019.9004797},
  file   = {:data/pdfs/Loper2019.pdf:PDF},
  hash_id        = {2dd89d58a4e1bffde4e6fbf2e24ac56a99504d9057a6ef15f964d1f665ab8070},
}

@InProceedings{Lopez2019,
  author = {Lopez, Eduardo Carbajal and Marmier, Francois and Fontanili, Franck},
  booktitle      = {2019 Winter Simulation Conference (WSC)},
  title  = {Bus Fleet Size Dimensioning In an International Airport Using Discrete Event Simulation},
  year   = {2019},
  pages  = {464–475},
  series = {WSC '19},
  abstract       = {The continuous growth of airport operations generates continuous infrastructure expansion needs at international airports. In some cases, in the face of limitations in short-term expansions, the immediate solution is the creation of remote positions that allow a greater number of flights to be attended but which require the use of bus fleets to attend passenger boarding and disembarking operations. The sizing of the buses will allow to reduce the waiting times of flights below the level of service required by international standards. For this purpose, the development of a discrete event simulation model integrated to the transactional flight database is proposed. In the literature, although many references to simulation models have been found in airports, they have not been specifically focused in order to size and manage the bus fleet. Simulation experiments of this model under the management of scenarios allows to identify the bus fleet size required.},
  doi    = {10.1109/wsc40007.2019.9004878},
  hash_id        = {1f1455e264adba8d3300be90132b70e7756a4f74a28ce9499950413482bd5512},
}

