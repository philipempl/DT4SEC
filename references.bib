}

@incollection{Lind2021,
  author = {Lind, Mikael and Ward, Robert and Jensen, Henrik Hvid and Chua, Chye Poh and Simha, André and Karlsson, Jörgen and Göthberg, Lena and Penttinen, Toni and Theodosiou, Despina Panayiotou},
