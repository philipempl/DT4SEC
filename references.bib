  volume         = {104},
  abstract       = {The heterogeneous requirements might lead to time-consuming and iterative design process. This paper intends to discuss the possibility of applying the generative design principle on the design of robotic manufacturing systems. According to the proposed design approach, designers need no longer design a specific robotic manufacturing system based on the clear specification of design requirements; instead, designers can propose the system family from the perspective of domain engineering, and the final architecture of the required robotic manufacturing system can be generated from the system family automatically with the continuous improvement of the heterogeneous requirement gathering result.},
  doi   = {https://doi.org/10.1016/j.procir.2021.11.314},
  hash_id        = {369f1ff39da0024f78ea07bd63b5026930fc3eb910de13ccc3a625d3ca41b20a},
  keywords       = {Heterogeneour requirements, generative design, robotic manufacturing systems, systems engineering, intelligent design},
}

