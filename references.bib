  hash_id        = {ff0256903dd444a545dae8255b329d25bd41eb8377e1cf21eed92d08a899b1e7},
  keywords       = {asset management, Asset Performance Management, digital twin, integrity operating windows, process operation, process safety},
}

@inbook{Zhu2021a,
