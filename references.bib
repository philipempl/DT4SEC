@Article{Greco2020,
  author         = {Greco, Alessandro and Caterino, Mario and Fera, Marcello and Gerbino, Salvatore},
  journal        = {Applied Sciences},
  title = {Digital Twin for Monitoring Ergonomics during Manufacturing Production},
