@InProceedings{Asaturova2020,
  author = {Asaturova, Yuliya},
  booktitle      = {Proceedings of the 2nd International Scientific Conference on Innovations in Digital Economy: SPBPU IDE-2020},
  title  = {Development of the Industrial Internet of Things in the Russian Economy},
  year   = {2020},
  series = {SPBPU IDE '20},
  abstract       = {The growing global competition for implementation of the "Industry 4.0" concept prioritizes issues connected with the shift of the national industry to a digital technology platform. The Industrial Internet of Things (IIoT), based on the Internet of things technology, is currently adopting the position of the main driving force of the industrial revolution, while being one of the key factors for improving production efficiency. Development of the IIoT projects will result in the improvement of industrial efficiency, thereby having an enormous effect on the world economy. As a result, potential consequences will not only enhance the industry itself, but also many other aspects of social life related to it, such as living standards, labor market, education, political system, and etc. This study observes the history and prerequisites for the Industrial Revolution, identifies its main characteristics, and concentrates on the most advanced technologies. The research considers foreign experience in implementation of state programs in the Industry 4.0; defines a role of the Internet of things as a technological basis for a new digital economy; studies the IIoT concept as a key factor in the evolution of the Industrial Revolution; examines main advantages of the IIoT, its impact and projected consequences for the world economy; provides examples of implementation of the IIoT projects in the Russian and foreign markets; analyzes specific characteristics of the IIoT development in the Russian economy, as well as the factors hindering implementation of the IIoT projects. As a result of this study, priority tasks and prospects for development of the Industrial Internet of Things were outlined and evaluated.},
  doi    = {10.1145/3444465.3444496},
  hash_id        = {6e184e589e0d6f948f9df448a30ce3e34d68aa4425cb9e9941e5c530a4344659},
  keywords       = {Internet of Things, the Fourth Industrial Revolution, digitalization; efficiency, Industrial Internet},
}

@InProceedings{Asaturova2020a,
  author = {Asaturova, Yuliya},
  booktitle      = {Proceedings of the International Scientific Conference - Digital Transformation on Manufacturing, Infrastructure and Service},
  title  = {Peculiarities of Development of Industrial Internet of Things in the context of Russian economy},
  year   = {2020},
  series = {DTMIS '20},
  abstract       = {The growing global competition for implementation of the "Industry 4.0" concept prioritizes issues connected with the shift of the national industry to a digital technology platform. The Industrial Internet of Things (IIoT), based on the Internet of things technology, is currently adopting the position of the main driving force of the industrial revolution, while being one of the key factors for improving production efficiency. The main aim of the study is to analyze factors hindering the implementation of the IIoT projects in Russia, and to develop the list of priority tasks that can accelerate the progress in the area of IIoT in Russia. An analysis of the specifics of implementing IIoT projects in the Russian economy determines the scientific novelty of the study. The methodological basis of this study is represented by the works of the leading Russian and foreign experts on statistics, market economy theory, financial and economic analysis, and modern information technologies for data storage and processing. In this study, the history and prerequisites for the development of the industrial revolution are traced. The experience of different countries in implementing state programs within the Industry 4.0 concept is investigated. The concept of the Industrial IoT as a key factor in a new stage of industrial revolution development is explored. The peculiarities of the development of the Industrial IoT in the context of the Russian economy are analyzed. Examples of project realization in the sphere of the Industrial IoT in Russian and world markets are demonstrated. The factors that constrain project implementation in the sphere of the Industrial IoT in Russia are revealed. As a result of the study, the main tasks for the successful development of the Industrial IoT and Industrial IoT projects in Russia are formulated.},
  doi    = {10.1145/3446434.3446446},
  hash_id        = {7702a903efd69c71f822ce0deeea9fd54a2e39b76b45742aa1d412d20778cd41},
  keywords       = {the Fourth Industrial Revolution, Industrial Internet, digitalization, Internet of Things, efficiency},
}

