@Article{Zhang2021g,
  author         = {Xianyu Zhang and Xinguo Ming},
  journal        = {Computers & Industrial Engineering},
  title = {A comprehensive industrial practice for Industrial Internet Platform (IIP): General model, reference architecture, and industrial verification},
  year  = {2021},
  pages = {107426},
  volume         = {158},
  abstract       = {With the upgrading of industrial era, the rise of industrial Internet environment and the development of intelligent technology, enterprises will pay more attention to the system platform, information sharing, network collaboration, personalized customization and recommendation of services when designing, implementing and operating an industrial information platform, which is called Industrial Internet Platform (IIP). In the existing literature, scholars focus on a local dot, such as: technology, function, elements and application based on IIP. However, there is a lack of an overall framework to study the top-level planning of IIP implementation from a systematic perspective. Moreover, there are few studies on the detailed path and steps of IIP implementation in a specific industry. The research objective is to study the general model, reference architecture, service evaluation index system, implementation path, application verification, etc. for IIP which meets the needs of industry on the basis of existing theory and industrial practice, and to provide reference for government and industry planning, design, implementation and promotion of IIP. From a case study, the comprehensive performance rate of comprehensive ability for IIP in a pharmaceutical manufacturing industry enterprise is calculated. It can reflect the comprehensive ability for IIP in enterprise from the ability of the IIP’s resource management, platform technology, industrial application, and etc. In addition, the general model, reference architecture, implementation path and service evaluation system of IIP can improve the enterprise's resource management ability, platform service ability, industrial application ability, and enhance the core value of the enterprise and increase benefits.},
  doi   = {https://doi.org/10.1016/j.cie.2021.107426},
  hash_id        = {c296cce72768018a59c738b9c6388d9c09eb1b8dc1939186b79966e640cd1aca},
  keywords       = {Industrial Internet, Industrial Internet platform, Industrial Internet of things (IIoT), Internet of things (IoT), Smart manufacturing, Intelligent manufacturing},
}

