@InProceedings{Karachay2020,
  author = {Karachay, Vitalina and Prokudin, Dmitry and Kononova, Olga and Pilyasova, Daria},
  booktitle      = {Proceedings of the 13th International Conference on Theory and Practice of Electronic Governance},
  title  = {Sociocultural information urban space in smart city context},
  year   = {2020},
  pages  = {568–575},
  series = {ICEGOV 2020},
  abstract       = {The paper presents the concept of Sociocultural Information Urban Space (SIUS) considered as a logical development of the intercultural-communicational, technical-resource approaches and social machine concept, proposed for studying, supplementation and identification the features, possibilities and base information flows of the smart city sociocultural face. The SIUS idea helps to recognize or improve the data driving city concept, because of smart and gamification technology use, so that greater range of digital services related to the sociocultural city facets can be provided to different stakeholders. The study includes exploring the interaction of pervasive processes of the Sociocultural Information Urban Space, specification of its architecture and concept. Based on the principles and practices of a pervasive approach, the main mechanisms of citizens' involvement into the active creative development of the SIUS are identified. Finally, the study discusses two application scenarios which demonstrate how pervasive processes of sustainable development and pervasive self-regulation mechanisms of the Sociocultural Information Urban Space can be used to meet the actual needs of citizens and decision-making.},
  doi    = {10.1145/3428502.3428588},
