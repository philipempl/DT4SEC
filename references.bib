@Article{Zeiringer2021,
  author         = {Zeiringer, Johannes},
  journal        = {PACIS 2021 Proceedings},
  title = {Tackling Knowledge Risks in Data-centric Collaborations: A Literature Review},
  year  = {2021},
  abstract       = {As digitization takes hold in all areas, collaboration within supply chains (SC) is also being disruptively changed. Data exchange in SC is increasing, and data-centric col-laborations emerge. Hand in hand new knowledge risks within the SC partnerships arise, due to the possibility of knowledge reengineering out of comprehensive datasets. This paper aims to focus on identifying challenges that pose knowledge risks and pos-sible actions to overcome them. Particularly, the focus is on knowledge risks emerging out of an organizational and technical perspective. These risks can also act as a barrier for technology adaption in industry, hence it is important to provide an overview of existing research, synthesize it with one another, and translate it to unexplored gaps. Therefore, a structured literature review is conducted. It is found that challenges are often related to topics of data security, data analytics, training, and trustful relation-ships. Furthermore, possible knowledge protection actions are shown.},
  hash_id        = {10e708596807d7208529cfa93f1fe2a7108473cf3eb4b5c263eacaba6f187fc0},
