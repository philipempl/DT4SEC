@incollection{Tardieu2020a,
  author = {Tardieu, Hubert and Daly, David and Esteban-LauzAn, José and Hall, John and Miller, George},
  booktitle      = {Deliberately Digital},
  title  = {Case Study 1: The Digital Transformation of Energy and Utilities (Focus on Electrification)},
  year   = {2020},
  pages  = {227--236},
  abstract       = {Global energy consumption continues to grow, but the concurrent need to reduce greenhouse gas emissions is forcing the adoption of a more sustainable energy model. The consequential increasing share of intermittent renewable energy sources, such as solar and wind, is creating further challenges for the stable production, transmission, and distribution of electricity. The historical pyramidal model is being progressively replaced by an ecosystem of multiple stakeholders, with the result that the balancing of energy supply and demand can no longer be managed by a centralized system: smart grids are rapidly becoming a prerequisite for renewable energy to become dominant. However, the current energy regulation is forcing players in the value chain to operate in isolation, while smart grids require data sharing between the stakeholders. Creating collaborative ecosystems between energy players is the only way to enter the next era for energy.},
  doi    = {10.1007/978-3-030-37955-1_22},
