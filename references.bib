@incollection{Kessler2020,
  author = {Kessler, Rudolf W. and Kessler, Waltraud},
  booktitle      = {Comprehensive Chemometrics (Second Edition)},
  title  = {Best Practice and Performance of Hardware in Process Analytical Technology (PAT)},
  year   = {2020},
  editor = {Steven Brown and Romà Tauler and Beata Walczak},
  pages  = {237--274},
  abstract       = {Quality by Design (QbD), Industrie 4.0 (i40) respectively the Industrial Internet of Things (IIoT) need context sensitive information for future smart manufacturing and processing. Data analytics together with intelligent process analytical technology sensors (PAT) are the enabling technologies to realize these concepts. This contribution tries to provide some hints and tricks for a best practice and performance of use in spectroscopic PAT-applications to increase the understanding between the data analyst and the spectroscopist. It should provide ideas to avoid pitfalls in data analytics but also increase the awareness of the spectroscopist for complex inline solutions. This contribution describes after an introduction of the new concepts the needs for future PAT systems and its relevance to the data scientists. A description of fundamental limitations of the spectroscopic sensors and a critical assessment of common mistakes as well as sources of perturbations should increase the interdisciplinary cooperation between the data analytic specialists and spectroscopists.},
