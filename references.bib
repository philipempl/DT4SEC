@Book{Rosian2021,
  author         = {Rosian, Marvin and Hagenhoff, Philipp and Otto, Boris},
  title = {Towards a Holistic Cloud Computing Taxonomy: Theoretical & Practical Findings},
  year  = {2021},
  abstract       = {Although the concepts of cloud computing are well known and used in almost every domain and by companies of all sizes, new ideas and technologies in this area are emerging almost daily. As a result, cloud users often struggle to obtain an up-to-date overview of the most important dimensions of cloud computing and the services that are offered by various cloud providers. Therefore, this paper develops a cloud computing taxonomy that is theoretically grounded by performing a structured literature review and empirically validated by conducting workshops with participants from industry and science. By describing the most important dimensions and characteristics of cloud computing in detail and by applying the taxonomy using three large cloud providers, we aim to give the reader a holistic overview of the cloud and demonstrate the relevance of cloud computing for digital innovation.},
  hash_id        = {cd344d9eddcbd7b9a509137f7068345fc1597422181ac42cc6516ba8e683330e},
