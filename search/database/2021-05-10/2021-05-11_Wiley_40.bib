@InBook{Furness2019,
  author    = {Furness, Zach},
  chapter   = {11},
  pages     = {287-299},
  publisher = {John Wiley & Sons, Ltd},
  title     = {Perspectives on Securing Cyber Physical Systems},
  year      = {2019},
  isbn      = {9781119552482},
  abstract  = {Summary Cyber Physical Systems (CPS) are gaining use within multiple industry sectors including health, energy, and transportation. While these systems afford greater functionality, they also create a heightened security risk due to their network-enabled capabilities, their reliance on software, and their direct interaction with physical systems. This chapter will provide a user perspective on the challenges in securing these systems, based on known vulnerabilities and attacks against CPS. It will also outline challenges and opportunities for the use of modeling and simulation in supporting improved security systems engineering of these systems.},
  booktitle = {Complexity Challenges in Cyber Physical Systems},
  doi       = {https://doi.org/10.1002/9781119552482.ch11},
  eprint    = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119552482.ch11},
  keywords  = {cyber physical systems, Internet of Things, modeling technology, security vulnerability, simulation technology},
  url       = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119552482.ch11},
}

@InBook{Mittal2019,
  author    = {Mittal, Saurabh and Tolk, Andreas},
  chapter   = {1},
  pages     = {1-25},
  publisher = {John Wiley & Sons, Ltd},
  title     = {The Complexity in Application of Modeling and Simulation for Cyber Physical Systems Engineering},
  year      = {2019},
  isbn      = {9781119552482},
  abstract  = {Summary Cyber Physical Systems (CPS) are hybrid systems comprising multiple components and many other combinations of various integrated but heterogeneous computational and physical capabilities. Todays embedded system such as iPhones and smart watches have integrated hardware and software solutions. There are engineering methodologies available to develop such embedded systems, focusing on the system. While their use at a personal level is much understood, what is less understood, and a topic of ongoing research is the question of what happens when such systems are deployed across multiple sectors, such as intelligent transportation systems, Internet of Things, smart medical devices, and many more, and used in activities not originally intended by the designers, for example, controlling the physical devices such as an automobile or an electricity power generation plant. This cross-sector deployment and the collective use of such systems introduces risk at the macro-system level that may have cascaded impacts in a highly networked environment, which further impact the constituent systems themselves. This is a major problem. One possibility to reduce the technical risk is to remotely control the systems in the cyber environments, but the sheer number of variables and possible situations introduce complexity at multiple scales. This complexity results in test plans with limited coverage. Additional cyber physical system related issues of intelligence, adaptation, autonomy, and security make the problem even worse. The proposed solution is the enhanced use of Modeling and Simulation (M\&S). The M\&S discipline has supported the development of complex systems since its inception. This chapter provides an overview on the M\&S methods and technologies that aid CPS engineering in the development and testing phases, and CPS governance when they are deployed in complex cyber environments.},
  booktitle = {Complexity Challenges in Cyber Physical Systems},
  doi       = {https://doi.org/10.1002/9781119552482.ch1},
  eprint    = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119552482.ch1},
  keywords  = {autonomy aspects, complex cyber environments, cyber physical systems engineering, modeling and simulation methods, systems theory},
  url       = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119552482.ch1},
}

@Article{Cardenas2018,
  author   = {Cárdenas, Jorge and Menéndez, David},
  journal  = {The Journal of Engineering},
  title    = {Internet of things: how the electrical grid can be controlled and managed in other dimensions},
  year     = {2018},
  number   = {15},
  pages    = {918-923},
  volume   = {2018},
  abstract = {The ‘Internet of things’ is the ‘new revolution’. During the past decades, there were major changes in the energy industry: the increased implementation of renewable generation, together with a greater use of electronic power elements in the network such as high-voltage DC links and the use of flexible AC transmission system and new market requirements, introduce new challenges and a need for more intelligent assets monitoring; and additional optimisation with a better balance in the combination of different energy sources and thus guaranteeing the reliability of supply and quality of energy; together with the identification of new business models that allow the integration of new technologies, neutralising cyber threats and ensure an adequate dissemination of knowledge to new generations. Companies are migrating to a more digital model based fundamentally on three pillars: communications, big data and artificial intelligence. The right combination of these three pillars will optimise the operation of the electrical system, reducing costs and polluting emissions, accelerating the adoption of non-polluting energies and renewable energies, while maximising economic performance and a closer way of interacting with users. The industrial Internet of things (IIoT) has been identified as one of the technologies with most potential impact in the next years.},
  doi      = {https://doi.org/10.1049/joe.2018.0194},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1049/joe.2018.0194},
  keywords = {flexible AC transmission systems, power engineering computing, power grids, Internet of Things, HVDC power transmission, power markets, power supply quality, power transmission reliability, power transmission economics, electricity supply industry, power transmission control, energy sector, IIoT, electrical system, artificial intelligence, big data, neutralising cyber threats, intelligent assets monitoring, flexible AC transmission system, high-voltage DC links, renewable generation, energy industry, electrical grid},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1049/joe.2018.0194},
}

@Article{Shi2020,
  author   = {Shi, Qiongfeng and Dong, Bowei and He, Tianyiyi and Sun, Zhongda and Zhu, Jianxiong and Zhang, Zixuan and Lee, Chengkuo},
  journal  = {InfoMat},
  title    = {Progress in wearable electronics/photonics—Moving toward the era of artificial intelligence and internet of things},
  year     = {2020},
  number   = {6},
  pages    = {1131-1162},
  volume   = {2},
  abstract = {Abstract The past few years have witnessed the significant impacts of wearable electronics/photonics on various aspects of our daily life, for example, healthcare monitoring and treatment, ambient monitoring, soft robotics, prosthetics, flexible display, communication, human-machine interactions, and so on. According to the development in recent years, the next-generation wearable electronics and photonics are advancing rapidly toward the era of artificial intelligence (AI) and internet of things (IoT), to achieve a higher level of comfort, convenience, connection, and intelligence. Herein, this review provides an opportune overview of the recent progress in wearable electronics, photonics, and systems, in terms of emerging materials, transducing mechanisms, structural configurations, applications, and their further integration with other technologies. First, development of general wearable electronics and photonics is summarized for the applications of physical sensing, chemical sensing, human-machine interaction, display, communication, and so on. Then self-sustainable wearable electronics/photonics and systems are discussed based on system integration with energy harvesting and storage technologies. Next, technology fusion of wearable systems and AI is reviewed, showing the emergence and rapid development of intelligent/smart systems. In the last section of this review, perspectives about the future development trends of the next-generation wearable electronics/photonics are provided, that is, toward multifunctional, self-sustainable, and intelligent wearable systems in the AI/IoT era.},
  doi      = {https://doi.org/10.1002/inf2.12122},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/inf2.12122},
  keywords = {artificial intelligence, energy harvesting, human-machine interface, internet of things, wearable electronics, wearable photonics},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1002/inf2.12122},
}

@Article{Melesse2021,
  author   = {Melesse, Tsega Y. and Di Pasquale, Valentina and Riemma, Stefano},
  journal  = {IET Collaborative Intelligent Manufacturing},
  title    = {Digital Twin models in industrial operations: State-of-the-art and future research directions},
  year     = {2021},
  number   = {1},
  pages    = {37-47},
  volume   = {3},
  abstract = {Abstract A Digital Twin is a virtual representation of a physical product, asset, process, system, or service that allows us to understand, predict, and optimise their performance for better business outcomes. Recently, the use of Digital Twin in industrial operations has attracted the attention of many scholars and industrial sectors. Despite this, there is still a need to identify its value in industrial operations mainly in production, predictive maintenance, and after-sales services. Similarly, the implementation of a Digital Twin still faces many challenges. In response, a systematic literature review and analysis of 41 papers published between 2016 and 11 July 2020 have been carried out to examine recently published works in the field. Future research directions in the area are also highlighted. The result reveals that, regardless of the challenges, the role of Digital Twin in the advancement of industrial operations, especially production and predictive maintenance is highly significant. However, its role in after-sales services remains limited. Insights are offered for research scholars, companies, and practitioners to understand the current state-of-the-art and challenges, and to indicate future research possibilities in the field.},
  doi      = {https://doi.org/10.1049/cim2.12010},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1049/cim2.12010},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1049/cim2.12010},
}

@InBook{Datta2021,
  author    = {Datta, Shoumen Palit Austin and Saleem, Tausifa Jan and Barati, Molood and López, María Victoria López and Furgala, Marie-Laure and Vanegas, Diana C. and Santucci, Gérald and Khargonekar, Pramod P. and McLamore, Eric S.},
  chapter   = {2},
  pages     = {7-88},
  publisher = {John Wiley & Sons, Ltd},
  title     = {Data, Analytics and Interoperability Between Systems (IoT) is Incongruous with the Economics of Technology},
  year      = {2021},
  isbn      = {9781119740780},
  abstract  = {Summary P3 is a petri dish brimming with questions, not answers, but suggestions, to explore. The aim is not to teach or pontificate but may swing the proverbial pendulum between science and engineering in the context of commercial and consumer services. The reader may ponder about the amorphous questions or wonder in confusion. We disrupt the status quo and indulge in orthogonal, nonlinear, and asymmetric information arbitrage which may not be correct. This is a seed, sterile unless cultivated. We aspire to inform that tools and data related to the affluent world are not a template to be “copied” or applied to systems in the remaining (80\%) parts of the world which suffer from economic constraints. We need different thinking that resists the inclination of the affluent 20\% of the world to treat the rest of the world (80\% of the population) as a market. The 80/20 concept evokes the Pareto [1] theme in P3 and the implication is that ideas may float between (porous) the 80/20 domains (partition).},
  booktitle = {Big Data Analytics for Internet of Things},
  doi       = {https://doi.org/10.1002/9781119740780.ch2},
  eprint    = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119740780.ch2},
  keywords  = {Porous Pareto partition, Internet of Things, Data, Analytics},
  url       = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119740780.ch2},
}

@Article{Senthilnathan2020,
  author   = {Senthilnathan, Karthikrajan and Annapoorani, K. Iyswarya},
  journal  = {International Transactions on Electrical Energy Systems},
  title    = {Resilient cyber physical infrastructure for single-phase dual inverter with sliding mode control},
  year     = {2020},
  note     = {e12173 2050-7038.12173},
  number   = {1},
  pages    = {e12173},
  volume   = {30},
  abstract = {Summary As the need in cyber infrastructure for electrical systems to realize the industry 4.0 revolution, this paper presents a configuration of two-switch dual-output voltage source inverter with integral sliding mode control (ISMC) and cyber twin approach to realize the performance of the system. The inverter has the capability of supplying two independent loads of equal voltage at the load end. The virtual model (cyber twin) is developed to realize the operation of the inverter (physical device) with a host system. The cyber-physical test bench is developed to evaluate the performance of the physical model (inverter prototype) with cyber-physical capabilities. The coupled computation of the virtual model and prototype model is executed to analyze the performance of the inverter.},
  doi      = {https://doi.org/10.1002/2050-7038.12173},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/2050-7038.12173},
  keywords = {cyber-physical systems, dual-output inverter, rapid control prototype, sliding mode control},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1002/2050-7038.12173},
}

