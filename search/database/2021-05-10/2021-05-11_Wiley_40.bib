@InBook{Soldani2020,
  author    = {Soldani, David and Illingworth, Sally A.},
  pages     = {1-38},
  publisher = {American Cancer Society},
  title     = {5G AI-Enabled Automation},
  year      = {2020},
  isbn      = {9781119471509},
  abstract  = {Abstract Artificial intelligence (AI) has been investigated for more than 60 years. With the development of computing power, innovative algorithms, and gathering of massive data, AI will be rejuvenated in the next decade and become one of the most important enabling technologies for 5G automation. How to use the powerful analysis, judgment, and prediction capabilities provided by the AI algorithms to enable 5G equipment, networks and service systems, and combine them with the planning, construction, maintenance, operation, and optimization of carriers' networks become an important topic in the ICT industry. Based on deep understanding, accumulated experience in telecom field, and long-term investment in AI strategies, many organizations have been introducing AI technology to telecom networks and contributing to development of standards essential for improving the quality and security of AI products and services, ensuring user safety and creating a fair and open industry ecosystem. 5G networks can be automatically deployed (automatic service deployment and automatic running) with the introduction of AI based on application scenarios. AI algorithms can also be used for self-healing (automatic fault recovery), self-optimization (network self-optimization) and creating autonomous 5G systems (self-evolving networks), which continuously improve network efficiency and reduce OPEX. This article aims at presenting the most relevant use cases, prominent research outcomes, latest standard developments and related standardization gaps, examples of AI strategy interpretation, AI solutions – including communication intelligent platform, AI model service, and deployment solution – and typical application scenarios. As a part of this framework, this article introduces the most important aspects of AI security, in terms of protecting the integrity and confidentiality of AI models and data, and thus preventing attackers from changing the inference results or stealing data.},
  booktitle = {Wiley 5G Ref},
  doi       = {https://doi.org/10.1002/9781119471509.w5GRef225},
  eprint    = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119471509.w5GRef225},
  keywords  = {Artificial Intelligence, machine learning, neural networks, autonomous networks, automation, autonomics, 5G},
  url       = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119471509.w5GRef225},
}

@Article{Geist2018,
  author   = {Geist, Verena and Natschläger, Christine and Illibauer, Christa and Schewe, Klaus-Dieter},
  journal  = {Journal of Software: Evolution and Process},
  title    = {Towards functional safety and security for adaptive and flexible business processes},
  year     = {2018},
  note     = {e1952 smr.1952},
  number   = {5},
  pages    = {e1952},
  volume   = {30},
  abstract = {Abstract Business process management (BPM) provides many benefits for a company including productivity, efficiency, compliance, risk management, consistency, repeatability, and measurability. Many of these aspects also ensure and improve functional safety, privacy, and security in process execution. However, managed business processes lack flexibility; ie, changing a business process requires more effort than ad hoc execution and adaptation. Thus, especially in small companies, the acceptance rate of managed business processes is low. So we claim that in current BPM approaches, the structuredness ensuring functional safety and security concepts contradicts with the objective for flexibility. The main goal of the AdaBPM project is to address this contradiction by providing a formal approach to handle advanced adaptations and exception handling in business processes. The technical objectives of the project include (1) the definition of a basic framework being capable of handling different levels of adaptivity and considering security and safety requirements at the same time, (2) a rigorous process specification language and model refinement methods, (3) static business process adaptations, and (4) dynamic (or ad hoc) adaptations. The result of our research is a general approach for flexible BPM combined with the possibility to nevertheless manage the process and define safety and security restrictions.},
  doi      = {https://doi.org/10.1002/smr.1952},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/smr.1952},
  keywords = {business process management, exception handling, fault tolerance, formal semantics, process adaptations, safety and security restrictions},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1002/smr.1952},
}

