@Article{Reis2020,
  author   = {Reis, Lúcio Henrik A. and Murillo Piedrahita, Andrés and Rueda, Sandra and Fernandes, Natália C. and Medeiros, Dianne S. V. and de Amorim, Marcelo Dias and Mattos, Diogo M. F.},
  journal  = {Transactions on Emerging Telecommunications Technologies},
  title    = {Unsupervised and incremental learning orchestration for cyber-physical security},
  year     = {2020},
  number   = {7},
  pages    = {e4011},
  volume   = {31},
  abstract = {Abstract Attacks on cyber-physical systems, such as nuclear and water treatment plants, have physical consequences that impact the lives of thousands of citizens. In such systems, it is mandatory to monitor the field network and detect potential threats before a problem occurs. This work proposes a hybrid approach that orchestrates unsupervised and incremental learning methods to detect threats that impact the control loops in a plant. We use online data processing to identify new attack vectors. We train the online incremental learning method as new attacks arrive. We also apply a one-class support vector machine to each monitored sensor or actuator to retrieve abnormal behaviors of their closed control loop. The proposed solution orchestrates the outputs from the two machine learning methods and alerts the system operators when it detects a threat. We evaluate the proposal on the Secure Water Treatment testbed dataset, and the results reveal that our proposal detects threats at more than 90\% precision and with accuracy higher than 95\%.},
  doi      = {https://doi.org/10.1002/ett.4011},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/ett.4011},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ett.4011},
}

@InBook{2020d,
  chapter   = {23},
  pages     = {627-637},
  publisher = {John Wiley & Sons, Ltd},
  title     = {THE FUTURE OF SYSTEMS ENGINEERING},
  year      = {2020},
  isbn      = {9781119516699},
  abstract  = {Summary Systems engineers may need to adjust their thinking and adapt to a new problem set injected into society as a result of changes to the system or disruptive events. Changes can include: change in user needs; system mission changes; competition and teaming changes; and changing threat performance. Disruptions may also cause the systems engineer to adapt in the environment and workplace such as disruptions in technology, processes, and people-system interfaces. Future systems engineering efforts may be applied to different domains other than the traditional defense and aerospace projects. These include healthcare, transportation, environment, cybersecurity and Internet of Things, and manufacturing. Systems engineers may need to reconsider the traditional modality of their education. Some of these areas for consideration are core systems engineering topics, systems thinking, and the transdisciplinary development of ideas.},
  booktitle = {Systems Engineering Principles and Practice},
  doi       = {https://doi.org/10.1002/9781119516699.ch23},
  eprint    = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119516699.ch23},
  keywords  = {cybersecurity, environment, healthcare, Internet of Things, manufacturing sector, systems engineering, transportation},
  url       = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119516699.ch23},
}

@InBook{2021a,
  chapter   = {18},
  pages     = {437-457},
  publisher = {John Wiley & Sons, Ltd},
  title     = {DIGITAL REEDINESS ASSESSMENT AND SIX-STEP DIGITALIZATION JOURNEY},
  year      = {2021},
  isbn      = {9781119634140},
  abstract  = {Summary The goal of digital readiness assessment is to determine the digital maturity of the organization in order to know where a company stands in the digital maturity and identify what actions to take to satisfy the requirements and where to start for an effective digital journey. The readiness assessment helps the organization to understand organization readiness for digital transformation given information technology infrastructure and operation technology/engineering technology capabilities. The digitalization readiness assessment plays an important role in building a long-term strategy for Industry 4.0. Digital twin is an effective example of IT and OT/ET integration because it is a virtual digital copy of a physical asset. The chapter presents a typical digitalization journey takes six steps: opportunity assessment, readiness assessment, define the use cases, design data architecture, implement the pilot program, and implement the digital platform at full scale.},
