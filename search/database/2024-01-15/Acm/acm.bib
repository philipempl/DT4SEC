numpages = {6},
keywords = {The whole life cycle of buildings, Digital technology, safety, CIM, Sustainability, BIM, VR, IoT Technologies},
location = {<conf-loc>, <city>Ho Chi Minh City</city>, <country>Vietnam</country>, </conf-loc>},
