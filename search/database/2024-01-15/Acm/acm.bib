@inproceedings{10.1145/3600160.3605095,
author = {Schauer, Stefan and Hadjina, Tamara and Damjanovic, Melita and Muñoz Navarro, Eva Maria and Hernandez Montesinos, Juan Jose and Hingant Gómez, Javier and Papadopoulos, Lazaros},
title = {Detecting a Complex Attack Scenario in an Airport: The PRAETORIAN Framework},
