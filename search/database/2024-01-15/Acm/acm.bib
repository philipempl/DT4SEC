publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3635175.3635214},
doi = {10.1145/3635175.3635214},
abstract = {Purpose: Artificial intelligence technology is one of the hot spots in the field of science and technology today, and it is widely used in various fields. Environmental protection is also one of the major challenges facing the world. How to use artificial intelligence technology to solve environmental problems has become a topic of great concern. Method: Using the low-carbon city pilot policy as a quasi natural experiment, the panel data of the Yangtze River Economic Belt from 2015 to 2022 was analyzed using artificial intelligence, big data algorithms, and dual difference models, this paper empirically evaluates the pollution reduction effect of industrial pollution emission level. Results: The empirical results indicate that low-carbon pilot cities have significantly reduced wastewater emissions and per capita wastewater emissions; the promotion effect of low-carbon pilot city construction on carbon emission performance is sustained and continuously enhanced over time; and most of the leading low-carbon cities which have entered the post industrialization stage have less resistance to policy implementation and greater pollution reduction effect. Based on the above results, policy recommendations are put forward in this paper. Conclusion: The first is to expand the pilot scope of low-carbon cities and build a sustainable energy ecosystem. The second is to comprehensively deepen the concept of low-carbon development and pay attention to the control of wastewater and other pollution emissions. And the third is to scientifically design the combination of policy tools according to local conditions.},
booktitle = {Proceedings of the 2023 8th International Conference on Intelligent Information Processing},
pages = {216–222},
numpages = {7},
location = {<conf-loc>, <city>Bucharest</city>, <country>Romania</country>, </conf-loc>},
series = {ICIIP '23}
}

@inproceedings{10.1145/3428502.3428588,
