abstract = {Co-simulation is an increasingly important topic concerning future Industry 4.0 architectures as well as a part of modern design engineering approaches and testing structures for real production environments. It is an arising well-used enabling technique and is beneficial for the simulation of a coupled system by composing the simulations of its single parts. In this context, standardized information exchange is a requirement among production entities and the whole supply chain. Information models become a fundamental part for describing semantic exchange of data in a standardized way. These models can be customized according to the needs of a specific company or departments. At the moment, the testing of the communication structure and the overall interoperability between the communication partners in a production line is not checked until the real commissioning and later stages of a project. For the functionality and quality of the later established architecture it is still helpful to test information models and their functions in a static as well as a dynamic way beforehand. Oftentimes, an intelligent system is missing that fulfills these requirements of a dynamic and flexible testing. This paper focusses therefore on a concept for a co-simulation environment that accompanies different stages in the planning and virtual commissioning process.},
booktitle = {2022 The 3rd International Conference on Industrial Engineering and Industrial Management},
pages = {271–276},
numpages = {6},
keywords = {Information Model, Machine Communication, Virtual Commissioning, Co-Simulation, Cyber Physical System},
location = {Barcelona, Spain},
series = {IEIM 2022}
}

@inproceedings{10.1145/3603165.3607382,
author = {Zhu, Zhui and Wang, Xu and Dang, Fan},
