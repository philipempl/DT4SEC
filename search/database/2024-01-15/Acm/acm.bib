@inproceedings{10.1145/3446434.3446446,
author = {Asaturova, Yuliya},
title = {Peculiarities of Development of Industrial Internet of Things in the Context of Russian Economy},
year = {2021},
isbn = {9781450388900},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3446434.3446446},
doi = {10.1145/3446434.3446446},
abstract = {The growing global competition for implementation of the "Industry 4.0" concept prioritizes issues connected with the shift of the national industry to a digital technology platform. The Industrial Internet of Things (IIoT), based on the Internet of things technology, is currently adopting the position of the main driving force of the industrial revolution, while being one of the key factors for improving production efficiency. The main aim of the study is to analyze factors hindering the implementation of the IIoT projects in Russia, and to develop the list of priority tasks that can accelerate the progress in the area of IIoT in Russia. An analysis of the specifics of implementing IIoT projects in the Russian economy determines the scientific novelty of the study. The methodological basis of this study is represented by the works of the leading Russian and foreign experts on statistics, market economy theory, financial and economic analysis, and modern information technologies for data storage and processing. In this study, the history and prerequisites for the development of the industrial revolution are traced. The experience of different countries in implementing state programs within the Industry 4.0 concept is investigated. The concept of the Industrial IoT as a key factor in a new stage of industrial revolution development is explored. The peculiarities of the development of the Industrial IoT in the context of the Russian economy are analyzed. Examples of project realization in the sphere of the Industrial IoT in Russian and world markets are demonstrated. The factors that constrain project implementation in the sphere of the Industrial IoT in Russia are revealed. As a result of the study, the main tasks for the successful development of the Industrial IoT and Industrial IoT projects in Russia are formulated.},
booktitle = {Proceedings of the International Scientific Conference - Digital Transformation on Manufacturing, Infrastructure and Service},
articleno = {39},
numpages = {7},
keywords = {the Fourth Industrial Revolution, digitalization, Internet of Things, Industrial Internet, efficiency},
location = {Saint Petersburg, Russian Federation},
series = {DTMIS '20}
}

