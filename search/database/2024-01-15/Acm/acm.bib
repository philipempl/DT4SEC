author = {Kurte, Kuldeep and Berres, Anne and Ravulaparthy, Srinath and Sanyal, Jibonananda and Thakur, Gautam},
title = {The 13th ACM SIGSPATIAL International Workshop on Computational Transportation Science (IWCTS 2020): Seattle, WA, USA - November 3, 2020},
