location = {<conf-loc>, <city>Guilin</city>, <country>China</country>, </conf-loc>},
series = {CSSE '22}
}

@inproceedings{10.1145/3607720.3607788,
author = {El-Ouardi, Yassine and Hasidi, Oussama and Khamis, Jakob and Abdelwahed, El Hassan and Qazdar, Aimad and Sauter, Stephan and Timmermann, Jens and Bendaouia, Ahmed},
