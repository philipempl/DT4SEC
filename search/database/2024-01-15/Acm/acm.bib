}

@inproceedings{10.1145/3551690.3551702,
author = {Taghipour, Atour and Lu, Xiaowen and Derradji, Meriem and Sow, Amadou Diude},
title = {The Impact of Digitalization on Supply Chain Management: A Literature Review},
year = {2022},
isbn = {9781450396493},
publisher = {Association for Computing Machinery},
