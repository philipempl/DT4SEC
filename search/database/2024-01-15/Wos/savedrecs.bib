@article{ WOS:001063179600001,
Author = {Wang, Gang and Zhou, Mingliang and Fang, Bin and Zhang, Yugui and Guan,
   Shouqin and Ruan, Bin and Li, Zelin},
Title = {Fast 3D Object Measurement Based on Point Cloud Modeling},
Journal = {INTERNATIONAL JOURNAL OF PATTERN RECOGNITION AND ARTIFICIAL INTELLIGENCE},
Year = {2023},
Volume = {37},
Number = {11},
Month = {SEP 15},
Abstract = {Automated object measurement is becoming increasingly important due to
   its ability to reduce manual costs, increase production efficiency, and
   minimize errors in various fields. In this paper, we present a novel
   approach to three-dimensional (3D) object measurement based on point
   cloud modeling. Our method introduces a fast point cloud modeling
   computation framework consisting of five stages: coordinate
   centralization, rotation and translation, noise filtering, plane
   projection, and geometric computation. Furthermore, we propose a fast
   convex hull optimization algorithm to reduce the high complexity problem
   of traditional convex hull calculation. Our extensive experiments
   demonstrate that our approach outperforms existing methods in terms of
   measurement error rate and time savings, with a maximum time saving of
   31.03\% under certain error conditions.},
