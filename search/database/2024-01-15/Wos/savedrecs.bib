@article{ WOS:001004228100001,
Author = {Auyeskhan, Ulanbek and Steed, Clint Alex and Park, Soohyung and Kim,
   Dong-Hyun and Jung, Im Doo and Kim, Namhun},
Title = {Virtual reality-based assembly-level design for additive manufacturing
   decision framework involving human aspects of design},
Journal = {JOURNAL OF COMPUTATIONAL DESIGN AND ENGINEERING},
Year = {2023},
