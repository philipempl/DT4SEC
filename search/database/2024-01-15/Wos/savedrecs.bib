@article{ WOS:000638402700053,
Author = {Dai, Yueyue and Zhang, Ke and Maharjan, Sabita and Zhang, Yan},
Title = {Deep Reinforcement Learning for Stochastic Computation Offloading in
   Digital Twin Networks},
