@article{ WOS:001033434600001,
Author = {Kumari, Deepika and Kumar, Pankaj and Prajapat, Sunil},
Title = {A blockchain assisted public auditing scheme for cloud-based digital
   twin healthcare services},
Journal = {CLUSTER COMPUTING-THE JOURNAL OF NETWORKS SOFTWARE TOOLS AND
   APPLICATIONS},
Year = {2023},
Month = {2023 JUL 19},
Abstract = {The convergence with the emerging digital twin technology has been
   envisaged as the looming of a smart cloud-based healthcare system.
   Precision clone simulation of the patients and medical service builds a
   bridge to the virtual medical world, enabling diagnosing, monitoring,
   and future prediction to offer enhanced medical services. Cloud services
   provider plays a crucial role in endowing a way to interlink the
   physical world to the virtual medical world through outsourced storage,
   a communication platform, reduced computational cost, and the relevance
   of hosting and other services over the internet offered by it. However,
   any alteration, deviation, or amendment of the physical patient's and
   its digital twin's stored data in the cloud storage has been one of the
   security liabilities. In this work, we propose a blockchain-assisted
   certificateless public auditing mechanism for the cloud-based digital
   twin healthcare networks to substantiate the data integrity in cloud
   storage. Blockchain in the proposed mechanism enables keeping the
   auditing process and outsourcing activities log. Meanwhile, it also
   enjoys the benefits of batch auditing and pairing-free operations. The
   proposed work is demonstrated to be provably secure and efficient in
   achieving security goals like storage correctness, privacy-preserving,
   and public verifiability. Furthermore, the computational overhead of the
   presented work is shown to be efficient with contrasted with some
   existing works.},
