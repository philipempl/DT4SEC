@article{ WOS:000967286900001,
Author = {Lin, Yijing and Gao, Zhipeng and Shi, Weisong and Wang, Qian and Li,
   Huangqi and Wang, Miaomiao and Yang, Yang and Rui, Lanlan},
Title = {A Novel Architecture Combining Oracle With Decentralized Learning for
   IIoT},
Journal = {IEEE INTERNET OF THINGS JOURNAL},
Year = {2023},
Volume = {10},
