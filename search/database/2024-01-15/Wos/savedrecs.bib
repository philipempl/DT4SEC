@article{ WOS:000904931000013,
Author = {Kong, Xiangjie and Wu, Yuhan and Wang, Hui and Xia, Feng},
Title = {Edge Computing for Internet of Everything: A Survey},
Journal = {IEEE INTERNET OF THINGS JOURNAL},
Year = {2022},
Volume = {9},
