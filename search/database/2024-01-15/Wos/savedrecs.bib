@article{ WOS:001068938900010,
Author = {Zhou, Haifeng and Li, Mohan and Sun, Yanbin and Yun, Lei and Tian,
   Zhihong},
Title = {Digital Twin-Based Cyber Range for Industrial Internet of Things},
